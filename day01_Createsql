--TABLO OLUSTURMA---
CREATE TABLE student 
(
id CHAR (4),
name VARCHAR(20),
age NUMBER

);

--VERI GIRISI--
INSERT INTO student VALUES ('1001','TARIK CINGOZ',5);
INSERT INTO student VALUES ('1002','ZEYNEP CINGOZ',1);

--tablodan veri sorgulama--
SELECT * FROM student;

--tablo silme--
DROP TABLE student;

--PARCALI VERI GIRISI---
INSERT INTO student (id,name) VALUES('1003','MURAT CINGOZ');

--veri tabaninizda urunler adinda bir tablo olusturalim ---

CREATE TABLE urunler 
(
urun_id NUMBER,
urun_name VARCHAR(50),
fiyat NUMBER
);
INSERT INTO urunler VALUES ('100','cips',5);
INSERT INTO urunler VALUES ('200','kola',6);

select * from urunler;